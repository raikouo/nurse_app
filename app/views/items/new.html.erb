 
<%= form_with model:@test, url: items_path, local: true do |f| %>

  <%# error%>
  <%= render 'shared/error_messages', model: f.object %>
  <%# /error%>
  画像<br>
  <%= f.file_field :image %><br>
  第◯◯回<br>
  <%= f.text_field :time %><br>
  番号<br>
  <%= f.text_field :number %><br>
  問題<br>
  <%= f.text_area :question %><br>
  選択肢<br>
  <%= f.text_area :choice %><br>
  答え<br>
  <%= f.text_field :answer %><br>
  カテゴリー<br>
  <%= f.text_field :category %><br>

  <%= f.submit "出題する" %>
  <%=link_to 'もどる', root_path%>

<% end %>